<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Emails</title>
</head>
<body>
  <h1>Emails</h1>
  <ul>
    <% for (const email of emails) { %>
      <li>
        <h3><%= email.subject %></h3>
        <p><%= email.body %></p>
        <p>Category: <%= email.category %></p>
      </li>
    <% } %>
  </ul>
</body>
</html>
